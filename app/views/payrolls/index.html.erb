      <tr>
          <th> Period end date </th>
          <th> Pay Date </th>
          <th> Check Totals </th>
          <th> Gross Earnings </th>
      </tr>
    </thead>
      <% @payrolls.each do |info| %>
        <tbody>
            <tr>
                <td><%=link_to "click", payroll_path(info)%></td>
                <td><%=@pay_date %></td>
