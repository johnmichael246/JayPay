<h2 class="col s4"> This is the Payroll index </h2>
<hr width=85%>
<div class="container">
  <a href="/payrolls/new" class="waves-effect waves-light btn green right">New Payroll</a>
  <table class = "bordered centered responsive-table">
    <thead>
        <tr>
            <th> First Name</th>
            <th> Last Name</th>
            <th> Pay Rate</th>
            <th> Reg Hours </th>
            <th> Overtime Rate</th>
            <th> Overtime Hours</th>
        </tr>
    </thead>
      <% @payroll.each do |info| %>
        <tbody>
          <tr>
            <td> <%= info.employee.first_name %> </td>
            <td> <%= info.employee.last_name %> </td>
            <td> $<%= info.employee.rate %> </td>
            <td> <%= emp.reg_hours %> </td>
            <td> <%= emp.ovr_hours %> </td>
          </tr>
        </tbody>
      <% end %>
  </table>
</div>