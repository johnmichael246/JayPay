<div>
<h4 class="flow-text">Always stay on track</h4>
</div>
<div class="container">
    <div class="row">
        <%= form_for @todo do |f| %>
            <%=button_tag "Add", :class=>"waves-effect waves-light btn", :type=>"Submit" %>
        <div class="input-field col s6 offset-s1">
            <%= f.label :list,:class=>"active" %>
            <%= f.text_field :list %>
        </div>
        <%end%>
    </div>
<table class = "bordered centered highlight responsive-table">   
    <p class="flow-text bold">Todo List</p>
        <% @todos.each do |list| %>
            <tbody>
                <tr>
                    <td><%=list.created_at.strftime('%m.%d.%Y') %></td>
                    <td> <%= list.list %> </td> 
                    <td> <%=link_to edit_todo_path(list), :class=>"waves-effect waves-light btn-flat" do%>
                        <i class="material-icons">mode_edit</i><%end%>
                        <%=link_to list, method: :delete, :class=>"waves-effect waves-light btn-flat" do %>
                        <i class="material-icons">delete_forever</i> <%end%></td>
                </tr>
            </tbody>
        <% end %>
</table>
</div>